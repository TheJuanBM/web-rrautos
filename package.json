{
  "name": "rr",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "build:prod": "astro build && npm run compress",
    "preview": "astro preview",
    "compress": "gzip -9 -r dist/ && brotli -q 11 -r dist/",
    "analyze": "astro build && npx vite-bundle-analyzer dist/",
    "astro": "astro",
    "lint": "eslint . --ext .js,.ts,.astro",
    "lint:fix": "eslint . --ext .js,.ts,.astro --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "npm run lint && npm run format:check",
    "fix": "npm run lint:fix && npm run format",
    "prepare": "husky"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "astro": "^5.13.10",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "eslint": "^9.36.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "terser": "^5.31.0",
    "vite-bundle-analyzer": "^0.18.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.config.js": [
      "prettier --write"
    ],
    "scripts/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "public/sw.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ],
    "*.astro": [
      "prettier --write"
    ]
  }
}
